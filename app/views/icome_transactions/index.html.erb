<nav id="nav">
  <div class="nav-list">
    <a href="<%= categories_path %>" class="back-btn">
      <i class="fa-solid fa-chevron-left"></i> 
    </a>  
    <h1 class="title">transactions</h1>
    <%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'btn btn-outline-light btn-sm float-start logout' %>
  </div>
</nav>
<div class="total-transaction-income">
  <strong>
  <%= @category.name %>
  </strong>
  <strong>
  $ <%= @category.icome_transactions.sum(:amount)%>
  </strong>
</div>
<p style="color: green"><%= notice %></p>

<div id="icome_transactions">
  <% @icome_transactions.each do |icome_transaction| %>
    <%= render icome_transaction %>
    <%= button_to "Destroy this icome transaction",[@category, icome_transaction], method: :delete %>
  <% end %>
</div>
<div class="bottom-btn">
<%= link_to "New transaction", new_category_icome_transaction_path, class: 'category-btn' %>
</div>